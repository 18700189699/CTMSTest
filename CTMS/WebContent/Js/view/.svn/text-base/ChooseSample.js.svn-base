    $.extend({
        customerOpen: function() {
        $("#SampleList").unbind('dialogbeforeclose');
        $("#SampleList").bind('dialogbeforeclose', function(event, ui) {

        $("#SampleID").attr("value", "00201");
        $("#SampleName").attr("value", "门禁系统");
        
         });

        $("#SampleList").dialog({
                bgiframe: true,
                width: 1000,
                height: 800,
                modal: true,
                resizable: false
            });

            $("#SampleList").dialog('open');

        },
        SampleOpenC: function() { $("#SampleList").dialog('open'); },
        SampleClose: function() { $("#SampleList").dialog('close'); },
        SampleHideen: function() { $("#SampleList").dialog("destroy"); }
    });
    
